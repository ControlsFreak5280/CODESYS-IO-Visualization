__DECLARATION__
TYPE RotorkValve_UDT :
STRUCT
	Description: WSTRING;				//A Description for this Device
	LoopNumber: WSTRING;				//The Engineering Loop Definition for this value
	IPAddress: WSTRING(20);				//Input: The IP Address of this Device
	CI: BOOL;							//Control Inhibit: Disable Communication with end device
	Status : DINT;						//Device Status Code

	// Modbus IO
	InputData: RotorkValve_MBIO_InputData_UDT;
	OutputData: RotorkValve_MBIO_OutputData_UDT;

