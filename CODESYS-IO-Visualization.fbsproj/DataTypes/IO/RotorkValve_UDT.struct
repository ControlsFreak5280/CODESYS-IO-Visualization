	SignalLossPosition: INT;					// Position Command on loss of comms

	// Slave Information
	SlaveState: UINT;
	SlaveError: BOOL;
	SlaveErrorCode: INT;
	
	SimulationEnable: BOOL := TRUE;
	Simulator: RotorkValve_Simulator_UDT;
