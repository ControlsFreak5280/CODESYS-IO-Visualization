	Running_Open: BOOL;
	Remote_Selected: BOOL;
	Local_Stop_Selected_Offline: BOOL;
	Local_Stop_Selected: BOOL;
	Thermostat_Tripped: BOOL;
	Monitor_Relay: BOOL;
	Valve_Obstructed: BOOL;
	Valve_Jammed: BOOL;
	Valve_Moving_By_Hand: BOOL;
	Moving_Inhibited_by_MIT: BOOL;
	Position_Control_Enabled: BOOL;
	EEPROM_Checksum_Failure: BOOL;
	
	InputReg01: WORD;
	Battery_Low: BOOL;
	Open_Interlock_Active: BOOL;
	Close_Interlock_Active: BOOL;
	DI_1: BOOL;
	DI_2: BOOL;
	DI_3: BOOL;
	DI_4: BOOL;
	Reserved_7: BOOL;
	Reserved_8: BOOL;
	Reserved_9: BOOL;
	Control_Contention: BOOL;
	Partial_Stroke_Test_in_Progress: BOOL;
	Partial_Stroke_Test_Error: BOOL;
	General_Alarm: BOOL;
	Reserved_14: BOOL;
	Reserved_15: BOOL;
	
